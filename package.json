    "sls:deploy": "nuxt build && sls deploy",
    "sls:remove": "sls remove"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/moment": "^1.5.0",
    "aws-serverless-express": "^3.3.6",
